{
  "private": true,
  "scripts": {
    "dev": "cross-env NODE_ENV=development babel-node server.js",
    "build": "next build",
    "start": "cross-env NODE_ENV=production babel-node server.js",
    "compile": "relay-compiler",
    "update-schema": "babel-node ./scripts/updateSchema.js",
    "lint": "eslint ./ --cache"
  },
  "relay": {
    "src": "./",
    "schema": "./data/schema.graphql",
    "language": "typescript",
    "artifactDirectory": "./src/__generated__/relay",
    "excludes": ["__generated__/**", "node_modules/**", ".next/**"]
  },
  "dependencies": {
    "styled-components": "6.1.1",
    "styled-system": "5.1.5",
    "classnames": "2.2.6",
    "es6-promise": "4.2.8",
    "express": "^4.16.4",
    "express-graphql": "^0.7.1",
    "graphql": "^15.5.0",
    "graphql-relay": "^0.6.0",
    "isomorphic-unfetch": "3.0.0",
    "isomorphic-fetch": "^2.1.1",
    "next": "14.0.3",
    "prop-types": "^15.7.2",
    "react": "^18.0.0",
    "react-dom": "^18.0.0",
    "relay-hooks": "9.1.0",
    "relay-runtime": "18.0.0",
    "whatwg-fetch": "3.0.0",
    "react-infinite-scroller": "1.2.6"
  },
  "resolutions": { "**/@types/react": "17.0.2", "**/@types/react-transition-group": "4.2.0" },
  "devDependencies": {
    "@babel/core": "^7.3.4",
    "@babel/node": "^7.2.2",
    "@babel/plugin-proposal-class-properties": "^7.3.4",
    "@babel/plugin-transform-runtime": "^7.3.4",
    "babel-plugin-styled-components": "^2.1.4",
    "@babel/preset-env": "^7.3.4",
    "@babel/preset-react": "^7.0.0",
    "@babel/runtime": "^7.15.4",
    "@types/node": "^12.7.12",
    "@types/react": "^18.0.0",
    "@types/react-dom": "^18.0.0",
    "@types/relay-runtime": "^14.0.0",
    "@zeit/next-typescript": "1.1.1",
    "babel-loader": "^8.0.5",
    "babel-plugin-relay": "13.0.1",
    "cross-env": "6.0.3",
    "relay-compiler": "18.0.0",
    "relay-compiler-language-typescript": "15.0.1",
    "typescript": "^4.2.4"
  }
